{
  "expo": {
    "name": "aplicacion",
    "slug": "aplicacion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono.ico",
    "scheme": "aplicacion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icono.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir a Talento acceder a tu ubicación para mostrarte profesionales cercanos.",
          "locationAlwaysPermission": "Permitir a Talento acceder a tu ubicación para mostrarte profesionales cercanos.",
          "locationWhenInUsePermission": "Permitir a Talento acceder a tu ubicación para mostrarte profesionales cercanos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}